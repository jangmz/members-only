<nav>
    <% if (locals.user) { %>
        <h3>Hello, <%= user.username %></h3>
    <% } %>
    <ul>
        <% if (locals.user) { %>
            <li><a href="/messages/new">New Message</a></li>
            <li><a href="/messages">Messages</a></li>
            <li><a href="/log-out">Log Out</a></li>
        <% } else { %>
            <li><a href="/sign-up">Sign Up</a></li>
            <li><a href="/log-in">Log In</a></li>
        <% } %>
    </ul>
</nav>